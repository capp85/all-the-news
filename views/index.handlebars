
{{#each articles}}

<div class="card text-gray text-center mb-5 mx-auto" style="max-width: 80%;">
  <div class="card-header mt-3 bg-dark">{{this.title}}</div>
  <div class="card-body">
    <h4 class="card-title"></h4>
    <p class="card-text">{{this.link}}</p>
        <button type="submit" class="btn btn-primary" href=""  data-target="{{this.id}}">Save Article </button>
  </div>
</div>

{{/each}}